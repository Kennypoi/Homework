
public class TestBubblesort {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//这是一个冒泡排序程序
		int []a={22,23,12,53,12};
		
		for (int i = 0; i < a.length-1;i++) {
			for (int j = 0; j < a.length-1-i; j++) {
				if (a[j]>a[j+1]) {
					int temp=a[j];
					a[j]=a[j+1];
					a[j+1]=temp;
				}
				
			}
			
		}
		for (int i : a) {
			System.out.println(i);
		}

	}
	

}
